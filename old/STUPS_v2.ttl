#################################################################
#    Classes
#################################################################

###  http://www.stups.fr/ontologies/2023/stups/Aspect
:Aspect rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:unionOf ( [ rdf:type owl:Restriction ;
                                              owl:onProperty :estAspectExterneDe ;
                                              owl:someValuesFrom :Echantillon
                                            ]
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty :estAspectInterneDe ;
                                              owl:someValuesFrom :Echantillon
                                            ]
                                          )
                            ] ;
        rdfs:comment "Un aspect permet de d’écrire l’aspect interne ou l’aspect externe d’un échantillon (Indéterminé, Billes, Cailloux, Comprimés, Cristaux, Gélules, Pâtes, Poudres, Résines, Timbres, Végétaux, Huiles, Liquides ou Substances Dissimulées)." .


###  http://www.stups.fr/ontologies/2023/stups/Base
:Base rdf:type owl:Class ;
      rdfs:subClassOf :FormeChimique .


###  http://www.stups.fr/ontologies/2023/stups/BaseEtHCL
:BaseEtHCL rdf:type owl:Class ;
           rdfs:subClassOf :FormeChimique .


###  http://www.stups.fr/ontologies/2023/stups/Billes
:Billes rdf:type owl:Class ;
        rdfs:subClassOf :Aspect .


###  http://www.stups.fr/ontologies/2023/stups/Cailloux
:Cailloux rdf:type owl:Class ;
          rdfs:subClassOf :Aspect .


###  http://www.stups.fr/ontologies/2023/stups/Chlorhydrate
:Chlorhydrate rdf:type owl:Class ;
              rdfs:subClassOf :FormeChimique .


###  http://www.stups.fr/ontologies/2023/stups/Comprimes
:Comprimes rdf:type owl:Class ;
           rdfs:subClassOf :Aspect .


###  http://www.stups.fr/ontologies/2023/stups/Cristaux
:Cristaux rdf:type owl:Class ;
          rdfs:subClassOf :Aspect .


###  http://www.stups.fr/ontologies/2023/stups/Departement
:Departement rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aVille ;
                                                          owl:someValuesFrom :Ville
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :estDansRegion ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Region
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:comment "Un département est dans une région et a une ou plusieurs villes." .


###  http://www.stups.fr/ontologies/2023/stups/Echantillon
:Echantillon rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aProduitCoupage ;
                                                          owl:someValuesFrom :ProduitCoupage
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :estProcheDe ;
                                                          owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Echantillon
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aAspectExterne ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Aspect
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aAspectInterne ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Aspect
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aPrincipeActif ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :PrincipeActif
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :aProfilChimique ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :ProfilChimique
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :provientDe ;
                                                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Prelevement
                                                        ]
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :estDansLot ;
                                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                          owl:onClass :Lot
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:comment """Un échantillon de drogue est extrait à partir d’un prélèvement d’un scellé. Un échantillon est caractérisé par un numéro d’échantillon et son type de drogue ('cannabis', 'cocaïne', 'divers' ou 'amphétamine et dérivés'). Un échantillon a des caractéristiques macroscopiques (aspect internes et externes, couleurs etc.), un principe actif et des produits de coupage. Les experts peuvent aussi donner des commentaires sur l’échantillon. De plus, un échantillon peut être regroupé avec d’autres échantillon dans un lot.
Dans le cas des stupéfiants sur lesquels une analyse de profilage chimique est réalisée, chaque échantillon est associé à un profil chimique.""" .


###  http://www.stups.fr/ontologies/2023/stups/FormeChimique
:FormeChimique rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :estFormeChimiqueDe ;
                                     owl:someValuesFrom :PrincipeActif
                                   ] ;
               rdfs:comment "Une forme chimique (Base, HCL, Base et HCL, Sulfate, Chlorhydrate, Phosphate ou Indéterminée) est associée à un principe actif pour décrire sa forme chimique." .


###  http://www.stups.fr/ontologies/2023/stups/Gelules
:Gelules rdf:type owl:Class ;
         rdfs:subClassOf :Aspect .


###  http://www.stups.fr/ontologies/2023/stups/HCL
:HCL rdf:type owl:Class ;
     rdfs:subClassOf :FormeChimique .


